#!/bin/sh
# prerm script for #PACKAGE#

set -e

case "$1" in
    remove|upgrade|deconfigure)
      # Rspamd web interface doesn't support symlinks and directory traversal
      rm /usr/share/rspamd/www/js/lib/bootstrap.min.js
      rm /usr/share/rspamd/www/js/lib/jquery-3.4.1.min.js
      rm /usr/share/rspamd/www/js/lib/require.min.js
    ;;

    failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
